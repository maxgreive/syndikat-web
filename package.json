{
  "name": "syndikat-web",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "scripts": {
    "build:svelte": "npm run build -C svelte-components",
    "build:jekyll": "bundle exec jekyll build",
    "build:critical": "node critical.js",
    "build": "npm run build:svelte && npm run build:jekyll && npm run build:critical",
    "dev": "concurrently \"npm run dev:svelte\" \"npm run dev:jekyll\"",
    "dev:jekyll": "bundle exec jekyll serve",
    "dev:svelte": "npm run --prefix svelte-components dev",
    "clean": "bundle exec jekyll clean",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepare": "husky"
  },
  "dependencies": {
    "@rollup/plugin-commonjs": "^24.0.0",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@rollup/plugin-replace": "^5.0.5",
    "@rollup/plugin-terser": "^0.4.0",
    "awesomplete": "^1.1.7",
    "concurrently": "^9.2.1",
    "critical": "^7.2.1",
    "husky": "^9.0.11",
    "rollup": "^3.15.0",
    "rollup-plugin-css-only": "^4.3.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^7.1.2",
    "sirv-cli": "^2.0.0",
    "svelte": "^3.55.0",
    "svelte-spa-router": "^4.0.1",
    "tippy.js": "^6.3.7"
  },
  "keywords": [],
